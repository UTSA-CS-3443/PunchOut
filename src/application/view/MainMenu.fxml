<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="MainMenuPane" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.TitleScreenController">
   <children>
      <Button fx:id="newGameBtn" layoutX="315.0" layoutY="400.0" mnemonicParsing="false" onAction="#newGame" prefHeight="69.0" prefWidth="168.0" text="New Game" textFill="#dd1515">
         <font>
            <Font name="Elephant" size="23.0" />
         </font></Button>
      <Button fx:id="continueBtn" layoutX="315.0" layoutY="496.0" mnemonicParsing="false" onAction="#toLeaderboard" prefHeight="69.0" prefWidth="168.0" text="Leaderboard" textFill="#75cd31">
         <font>
            <Font name="Elephant" size="18.0" />
         </font></Button>
      <ImageView fitHeight="235.0" fitWidth="496.0" layoutX="152.0" layoutY="118.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Punch-Out_series_logo.png" />
         </image></ImageView>
      <Label layoutX="309.0" layoutY="651.0" text="Volume Controls" visible="false">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </Label>
      <Button layoutX="417.0" layoutY="723.0" mnemonicParsing="false" onAction="#volDown" text="Down" visible="false" />
      <Button layoutX="332.0" layoutY="723.0" mnemonicParsing="false" onAction="#volUp" prefHeight="25.0" prefWidth="47.0" text="Up" visible="false" />
   </children>
</AnchorPane>
